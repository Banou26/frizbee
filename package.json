{
  "name": "frizbee-wasm",
  "version": "0.5.0",
  "description": "Fast fuzzy matching via SIMD smith waterman - WASM package for browser use",
  "main": "pkg/frizbee_wasm.js",
  "types": "pkg/frizbee_wasm.d.ts",
  "files": [
    "pkg/**/*"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg --features wasm",
    "build:node": "wasm-pack build --target nodejs --out-dir pkg-node --features wasm",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg-bundler --features wasm"
  },
  "keywords": [
    "fuzzy",
    "search",
    "matching",
    "wasm",
    "fzf",
    "fzy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saghen/frizbee"
  }
}